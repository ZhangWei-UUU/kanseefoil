{
    "presets": [
      ["@babel/env"],
      "next/babel"
    ],
    "plugins": [
      ["@babel/plugin-proposal-decorators", { "legacy": true }],
      ["@babel/plugin-proposal-class-properties", { "loose": true }],
      ["styled-components", { "ssr": true, "displayName": true, "preprocess": false } ],
      ["import",
        {
          "libraryName": "antd",
          "libraryDirectory": "lib",
          "style":"css"
        }
      ]
    ]
  }